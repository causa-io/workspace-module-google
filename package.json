{
  "name": "@causa/workspace-google",
  "version": "0.11.0",
  "description": "The Causa workspace module providing many functionalities related to GCP and its services.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/causa-io/workspace-module-google.git"
  },
  "license": "ISC",
  "type": "module",
  "engines": {
    "node": ">=20"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js"
  },
  "files": [
    "dist/",
    "LICENSE.md",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "prebuild": "rimraf ./dist",
    "build": "tsc -p tsconfig.build.json",
    "postbuild": "copyfiles -u 1 src/**/assets/**/* dist/",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint \"src/**/*.ts\"",
    "test": "NODE_OPTIONS=\"--experimental-vm-modules --no-warnings=ExperimentalWarning\" jest",
    "test:cov": "npm run test -- --coverage"
  },
  "dependencies": {
    "@causa/cli": ">= 0.6.4 < 1.0.0",
    "@causa/workspace": ">= 0.19.1 < 1.0.0",
    "@causa/workspace-core": ">= 0.25.2 < 1.0.0",
    "@causa/workspace-typescript": ">= 0.15.2 < 1.0.0",
    "@google-cloud/apikeys": "^2.2.1",
    "@google-cloud/bigquery": "^8.1.1",
    "@google-cloud/iam-credentials": "^4.2.1",
    "@google-cloud/pubsub": "^5.2.2",
    "@google-cloud/resource-manager": "^6.2.1",
    "@google-cloud/run": "^3.1.0",
    "@google-cloud/secret-manager": "^6.1.1",
    "@google-cloud/service-usage": "^4.2.1",
    "@google-cloud/spanner": "8.4.0",
    "@google-cloud/storage": "^7.18.0",
    "class-validator": "^0.14.3",
    "firebase": "^12.7.0",
    "firebase-admin": "^13.6.0",
    "globby": "^16.1.0",
    "google-auth-library": "^10.5.0",
    "google-gax": "5.0.3",
    "googleapis": "^170.0.0",
    "micromatch": "^4.0.8",
    "pino": "^10.2.0",
    "quicktype-core": "^23.2.6"
  },
  "devDependencies": {
    "@swc/core": "^1.15.8",
    "@swc/jest": "^0.2.39",
    "@tsconfig/node20": "^20.1.8",
    "@types/jest": "^30.0.0",
    "@types/micromatch": "^4.0.10",
    "@types/node": "^22.19.6",
    "copyfiles": "^2.4.1",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "jest": "^30.2.0",
    "jest-extended": "^7.0.0",
    "rimraf": "^6.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.53.0"
  }
}
